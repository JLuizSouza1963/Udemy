	public static void main(String[] args) {
		
		LocalDate d04 = LocalDate.parse("2024-08-28");
		LocalDateTime d05 = LocalDateTime.parse("2024-08-28T20:59:00");
		Instant d06 = Instant.parse("2024-08-28T20:59:00Z");
		
		
		
	}

}
